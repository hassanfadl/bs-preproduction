"                                        Tax\n"
"                                    </th>\n"
"                                </tr>\n"
"                            </thead>\n"
"                            <tbody class=\"sale_tbody\">\n"
"                                % for line in object.order_line:\n"
"                                <tr>\n"
"                                    <td>\n"
"                                        ${line.name}\n"
"                                    </td>\n"
"                                    <td>\n"
"                                        ${line.product_uom_qty}\n"
"                                    </td>\n"
"                                    <td>\n"
"                                        ${line.price_unit}\n"
"                                    </td>\n"
"                                    <td>\n"
"                                       % if line.tax_id:\n"
"                                           ${line.tax_id[0].amount}\n"
"                                       % endif\n"
"                                    </td>\n"
"                                </tr>\n"
"                                % endfor\n"
